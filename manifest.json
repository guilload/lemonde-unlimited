{
  "name": "lemonde unlimited",
  "description": "Remove the monthly limit of 15 free articles.",
  "version": "0.1",
  "background": {"page": "background.html"},
  "manifest_version": 2,

  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },

  "content_scripts": [ {
    "js": [ "jquery-2.1.1.min.js", "background.js" ],
    "matches": [ "http://www.lemonde.fr/*"]
  }]
}
